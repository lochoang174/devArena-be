    "mongoose": "^8.8.4",
    "ms": "^2.1.3",
    "nodemailer": "^6.9.16",
    "passport": "^0.7.0",
    "passport-discord": "^0.1.4",
    "passport-github2": "^0.1.12",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pidusage": "^3.0.2",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1",
    "serve-static": "^1.16.2"
  },
  "devDependencies": {
