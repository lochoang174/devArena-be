{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/main/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/main/tsconfig.app.json"
  },
  "projects": {
    "common": {
      "type": "library",
      "root": "libs/common",
      "entryFile": "index",
      "sourceRoot": "libs/common/src",
      "compilerOptions": {
        "tsConfigPath": "libs/common/tsconfig.lib.json"
      }
    },
    "main": {
      "type": "application",
      "root": "apps/main",
      "entryFile": "main",
      "sourceRoot": "apps/main/src",
      "compilerOptions": {
        "tsConfigPath": "apps/main/tsconfig.app.json"
      }
    },
    "compile": {
      "type": "application",
      "root": "apps/compile",
      "entryFile": "main",
      "sourceRoot": "apps/compile/src",
      "compilerOptions": {
        "tsConfigPath": "apps/compile/tsconfig.app.json"
      }
    },
    "contest": {
      "type": "application",
      "root": "apps/contest",
      "entryFile": "main",
      "sourceRoot": "apps/contest/src",
      "compilerOptions": {
        "tsConfigPath": "apps/contest/tsconfig.app.json"
      }
    }
  },
  "monorepo": true,
  "root": "apps/main"
}